var style = require('p_./miniprogram_npm/xft-weapp/wxs/style.wxs');
var addUnit = require('p_./miniprogram_npm/xft-weapp/wxs/add-unit.wxs');

function titleStyle(data) {
    return (style([({
        'max-width': addUnit(data.titleWidth),
        'min-width': addUnit(data.titleWidth),
    }), data.titleStyle]))
};
module.exports = ({
    titleStyle: titleStyle,
});