.history {
    min-height: 100vh;
    width: 100vw;
    overflow: hidden;
}

.history::after {
    content: "";
    width: 750rpx;
    height: 830rpx;
    z-index: -1;
    overflow: hidden;
    position: relative;
    background: url("https://file.91xft.cn:9002/static/whyStatic/my-bg.png") no-repeat;
    background-size: 100%;
    margin-top: -50rpx;
    position: absolute;
    top: 0;
    left: 0;
}

.history-item {
    width: 686rpx;
    height: 189rpx;
    position: relative;
    overflow: hidden;
    padding: 0 48rpx;
    box-sizing: border-box;
    margin: 30rpx auto 0;
}

.history-item image {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.history-item__tag {
    width: 111rpx;
    height: 40rpx;
    font-size: 24rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: #fefefe;
    background: #999;
    border-radius: 0 0rpx 20rpx 0;
    text-align: center;
    position: absolute;
    left: 0;
}

.history-item__title {
    overflow: hidden;
    margin-top: 83rpx;
    width: 100%;
}

.history-item__title>view:nth-child(1) {
    font-size: 36rpx;
    font-family: DIN;
    font-weight: 500;
    line-height: 1;
    float: left;
    color: #333;
}

.history-item__title>view:nth-child(2) {
    font-size: 26rpx;
    line-height: 40rpx;
    font-family: DIN;
    float: right;
    font-weight: 500;
    color: #666;
}

.history-item__bottom {
    margin-top: 12rpx;
    overflow: hidden;
    line-height: 28rpx;
}

.history-item__bottom>view:nth-child(2) {
    font-size: 28rpx;
    font-family: PingFang SC;
    font-weight: 400;
    float: right;
    color: #999;
}

.history-item__bottom>view:nth-child(1) {
    font-size: 26rpx;
    font-family: PingFang SC;
    float: left;
    font-weight: 400;
    color: #999;
}

.myorder-none {
    text-align: center;
    height: 100vh;
    width: 100vw;
    overflow: hidden;
}

.myorder-none>image {
    width: 554rpx;
    height: 304rpx;
    position: absolute;
    top: 50%;
    left: 0;
    right: 0;
    margin: -260rpx auto 0;
}

.myorder-none>view {
    font-size: 30rpx;
    font-weight: 400;
    color: #999;
    line-height: 1;
    position: absolute;
    width: 100%;
    top: 50%;
    margin-top: 80rpx;
}

.myorder-none>view,.order-title {
    font-family: PingFang SC;
    text-align: center;
}

.order-title {
    position: relative;
    font-size: 37rpx;
    font-weight: 500;
    color: #333;
    height: 62rpx;
    line-height: 62rpx;
    margin-top: 50rpx;
    z-index: 10;
    margin-bottom: 60rpx;
}

.order-title view {
    width: 80rpx;
    height: 80rpx;
    position: absolute;
    left: 20rpx;
    top: 0rpx;
}

.order-title image {
    position: absolute;
    top: 8rpx;
    left: 20rpx;
    width: 48rpx;
    height: 48rpx;
}

.order-title .home-back {
    width: 60rpx;
    height: 60rpx;
    margin-top: -10rpx;
}