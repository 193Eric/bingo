.myorder-header {
    width: 750rpx;
    height: 340rpx;
    overflow: hidden;
    position: relative;
    z-index: 2;
}

.myorder::after {
    content: "";
    width: 750rpx;
    height: 830rpx;
    overflow: hidden;
    position: relative;
    background: url("https://file.91xft.cn:9002/static/whyStatic/my-bg.png") no-repeat;
    background-size: 100%;
    margin-top: -50rpx;
    position: absolute;
    top: 0;
    left: 0;
}

.red-header {
    background: #c8343a;
}

.view-transparent {
    background: transparent!important;
}

.myorder {
    overflow: hidden;
    min-height: 100vh;
    background: #f5f5f5;
    width: 100vw;
}

.show-bg {
    background: url("https://file.91xft.cn:9002/static/whyStatic/order-bg.png") no-repeat;
    background-size: 100%;
}

.myorder>image {
    height: 1169rpx;
    width: 100vw;
    position: absolute;
    top: 0;
    z-index: -1;
}

.myorder-header>view {
    margin-top: 183rpx;
}

.myorder-header__logo {
    width: 120rpx;
    border: 1px solid #0237ff;
    height: 120rpx;
    margin-left: 32rpx;
    margin-right: 36rpx;
    float: left;
    position: relative;
}

.myorder-header__logo>view {
    position: absolute;
    width: 13rpx;
    height: 13rpx;
    background: #0237ff;
    border-radius: 50%;
}

.myorder-header__logo>view:nth-child(1) {
    left: -7rpx;
    top: -7rpx;
}

.myorder-header__logo>view:nth-child(2) {
    right: -7rpx;
    top: -7rpx;
}

.myorder-header__logo>view:nth-child(3) {
    right: -7rpx;
    bottom: -7rpx;
}

.myorder-header__logo>view:nth-child(4) {
    left: -7rpx;
    bottom: -7rpx;
}

.myorder-header__logo image {
    width: 100%;
    border-radius: 50%;
    height: 100%;
    margin-right: 17rpx;
}

.myorder-header__title {
    float: left;
}

.myorder-header__title>view:nth-child(1) {
    font-size: 32rpx;
    font-family: PingFang SC;
    font-weight: 500;
    color: #333;
    margin-top: 21rpx;
    line-height: 36rpx;
}

.myorder-header__title>view:nth-child(2) {
    font-size: 24rpx;
    line-height: 26rpx;
    margin-top: 21rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: #999;
}

.myorder-header__list {
    width: 183rpx;
    height: 66rpx;
    margin-top: 55rpx;
    font-size: 26rpx;
    font-family: PingFangSC-Medium,PingFang SC;
    font-weight: 500;
    color: #0237ff;
    text-align: center;
    line-height: 66rpx;
    float: right;
    margin-right: 60rpx;
    background: url("https://file.91xft.cn:9002/static/whyStatic/history-btn.png") no-repeat;
    background-size: 100%;
}

.myorder-none {
    text-align: center;
    margin-top: 100rpx;
}

.myorder-none>image {
    width: 350rpx;
    height: 294rpx;
    margin: 72rpx autp 0;
}

.myorder-none>view {
    font-size: 30rpx;
    font-weight: 400;
    color: #999;
    margin-top: 28rpx;
}

.card-title,.myorder-none>view {
    font-family: PingFang SC;
    text-align: center;
    line-height: 1;
}

.card-title {
    font-size: 40rpx;
    font-weight: 500;
    color: #0237ff;
    margin-top: 120rpx;
}

.card-num {
    font-size: 60rpx;
    font-family: DIN;
    font-weight: 500;
    line-height: 60rpx;
    text-align: center;
    margin-top: 30rpx;
    color: #ff0082;
}

.card-num-append {
    font-size: 24rpx;
    font-family: PingFangSC-Regular,PingFang SC;
    font-weight: 400;
    color: #999;
    position: relative;
    line-height: 1;
    top: 10rpx;
}

.card-message {
    display: flex;
    width: 100%;
    margin-top: 51rpx;
}

.card-message__div {
    flex: 1;
    text-align: center;
    position: relative;
}

.card-message__div:nth-child(1)::after {
    content: "";
    position: absolute;
    background: #0237ff;
    width: 1rpx;
    height: 40rpx;
    line-height: 1;
    top: 15rpx;
    right: 0;
}

.card-message__div>view:nth-child(1) {
    font-size: 32rpx;
    font-family: PingFang SC;
    font-weight: 500;
    color: #333;
    line-height: 1;
}

.card-message__div>view:nth-child(2) {
    font-size: 28rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: #999;
    margin-top: 16rpx;
}

.myorder-card {
    width: 686rpx;
    height: 1120rpx;
    position: relative;
    overflow: hidden;
}

.myorder-card .image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
}

.myorder-card__bottom {
    margin-top: 83rpx;
    padding: 0 60rpx;
}

.myorder-card__item {
    width: 100%;
    overflow: hidden;
    line-height: 1;
    margin-bottom: 28rpx;
}

.myorder-card__bottom .myorder-card__item>view:nth-child(1) {
    font-size: 24rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: #999;
    text-align: left;
    float: left;
}

.myorder-card__bottom .myorder-card__item>view:nth-child(2) {
    font-size: 24rpx;
    font-family: DIN;
    font-weight: 400;
    color: #999;
    float: right;
    text-align: right;
    width: 400rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.zoom-out {
    transform: scale(.9);
    transition: all .4s ease-out 0s;
    z-index: 1;
    opacity: .6;
}

.zoom-in {
    transform: scale(1);
    z-index: 100;
    transition: all .4s ease-in 0s;
}

.dot-list {
    text-align: center;
    position: absolute;
    top: 1320rpx;
    left: 0;
    right: 0;
    margin: 0 auto;
}

.myorder .dot {
    display: inline-block;
    width: 20rpx;
    height: 10rpx;
    background: #ccc;
    border-radius: 5rpx;
    z-index: 100;
    margin: 0 8rpx;
}

.myorder .dot-active {
    width: 40rpx;
    height: 10rpx;
    background: #0237ff;
    border-radius: 5rpx;
}

.myorder .wx-swiper-dots .wx-swiper-dots-horizontal {
    margin-bottom: -8rpx;
}

button::after {
    border: none;
}

.login-btn {
    padding: 0;
    font-size: 24rpx!important;
    line-height: 26rpx;
    margin: 21rpx 0 0;
    font-family: PingFang SC;
    font-weight: 400;
    color: #fff;
    background: inherit;
    background-color: inherit;
    border-radius: inherit;
    font-size: inherit;
    text-align: inherit;
    line-height: inherit;
    background: transparent;
    border: none;
}

.order-detail__append {
    width: 100%;
    overflow: hidden;
    margin: 20rpx auto 0;
    float: left;
    box-sizing: border-box;
    padding: 20rpx 2rpx 20rpx 20rpx;
}

.append-tips {
    font-size: 24rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: #0237ff;
    float: left;
    line-height: 34rpx;
    height: 34rpx;
}

.append-tips image {
    margin-right: 10rpx;
}

.append-li image,.append-tips image {
    width: 27rpx;
    height: 26rpx;
    position: relative;
    top: 4rpx;
}

.append-li image {
    left: -4rpx;
}

.append-li {
    font-size: 20rpx;
    line-height: 34rpx;
    height: 34rpx;
    font-weight: 400;
    color: #0237ff;
    float: left;
}

.append-detail,.append-li {
    font-family: PingFang SC;
    margin-top: 10rpx;
}

.append-detail {
    font-size: 22rpx;
    font-weight: 300;
    color: #999;
    line-height: 28rpx;
    box-sizing: border-box;
    padding-right: 0;
}

.bottom {
    float: left;
    width: 100%;
    margin-right: -38rpx;
    overflow: hidden;
    margin-top: 20rpx;
}

.bottom-btn {
    width: 277rpx;
    height: 117prx;
    font-size: 30rpx;
    background: url("https://file.91xft.cn:9002/static/whyStatic/button-red.png") no-repeat;
    background-size: 100%;
    float: right;
    font-family: PingFang SC;
    font-weight: 500;
    text-align: center;
    line-height: 117rpx;
    margin-right: -18rpx;
    color: #ff0082;
}

.order-detail__top {
    background: url("https://file.91xft.cn:9002/static/whyStatic/myorder-bg.png") no-repeat;
    background-size: 100%;
    float: left;
    width: 100%;
    height: 703rpx;
}

.order-detail__append {
    background: url("https://file.91xft.cn:9002/static/whyStatic/dikuang.png") no-repeat;
    background-size: 100%;
}