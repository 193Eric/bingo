.outsidePading {
    padding: 0 30rpx;
}

.flex,.flex-around,.flex-between {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.flex,.flex-around {
    justify-content: space-around;
}

.flex {
    justify-content: flex-start;
    -webkit-align-items: center;
}

.show_address {
    padding: 30rpx 80rpx 30rpx 0;
    display: none;
}

.show_address.show {
    display: block;
}

.address_container {
    border-radius: 16rpx;
    background: #fff;
    padding: 30rpx 30rpx 0;
    margin-bottom: 24rpx;
    position: relative;
}

.address_container .no_address_box {
    background: #fff;
    padding: 10rpx 0;
}

.address_container .address_title {
    line-height: 80rpx;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    font-weight: 400;
    color: #333;
    background: #fff;
    padding: 0 30rpx;
    border-bottom: 2rpx solid #eaeaea;
    position: relative;
}

.address_container .address_title::before {
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 6rpx;
    background: #ff9600;
}

.address_container .address_box {
    background: #fff;
    position: relative;
    padding: 20rpx 0;
}

.address_container .address_box .select_flag {
    float: left;
    margin: 6rpx 16rpx 0 0;
    width: 36rpx;
    height: 36rpx;
    border-radius: 50%;
    border: 2rpx solid #e4e4e4;
}

.address_container .address_box .address_type_flag {
    font-weight: 600;
    line-height: 48rpx;
}

.address_container .address_box.selected .select_flag {
    border: 0;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
}

.address_container .address_box.selected .address_type_flag {
    color: #fabe39;
}

.address_container .address_box.selected .address_detail,.address_container .address_box.selected .contacts_detail {
    display: block;
}

.address_container .detail_main {
    margin-right: 50rpx;
}

.address_container .address_detail {
    font-size: 30rpx;
    color: #333;
    font-weight: 600;
}

.address_container .contacts_detail {
    font-size: 28rpx;
    color: #333;
    padding-top: 10rpx;
}

.address_container .contacts_detail .contacts_name {
    padding-right: 20rpx;
}

.address_container .shelf_address {
    padding: 20rpx 30rpx;
}

.address_container .shelf_address .top_dash {
    height: 0;
    position: relative;
    top: -20rpx;
    border-top: 2rpx dashed #f3f5f7;
}

.address_container .shelf_address .detail_main {
    margin: 0;
}

.address_container .change_address_btn {
    position: absolute;
    width: 28rpx;
    height: 28rpx;
    top: 120rpx;
    right: 24rpx;
    background: url(data:image/png;base64,) no-repeat center right;
    background-size: 100% 100%;
}

.address_container .no_address_info {
    background: url(data:image/png;base64,) no-repeat left center;
    background-size: 32rpx 40rpx;
    padding-left: 48rpx;
    font-weight: 600;
    font-size: 30rpx;
    color: #333;
}

.address_switch_btn_container {
    height: 56rpx;
    background: #ffeec7;
    border-radius: 26rpx;
    position: relative;
    display: inline-block;
}

.address_switch_btn_container.has-disabled {
    background: #eaeaea;
}

.address_switch_btn_container>.span {
    display: inline-block;
    width: 160rpx;
    height: 100%;
    top: 0;
    font-size: 28rpx;
    color: #fabe39;
    font-family: PingFangSC-Regular;
    line-height: 56rpx;
    border-radius: 26rpx;
    text-align: center;
    font-weight: 500;
    box-sizing: border-box;
}

.address_switch_btn_container>.span.disabled {
    color: #cacaca;
    background: #eaeaea;
}

.address_switch_btn_container>.span.selected {
    font-size: 28rpx;
    color: #0d0d0d;
    background: #fabe39;
}

.address_switch_btn_container .send_btn {
    left: 0;
}

.address_switch_btn_container .self_get_btn {
    right: 0;
}

.address_w_box-wrapper {
    display: flex;
}

.address_w_box-wrapper .address_w_box {
    flex: 1;
    width: 0;
}

.address_w_box-wrapper .address_w_icon {
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
    background-repeat: no-repeat;
    width: 44rpx;
    height: 44rpx;
    margin-right: 16rpx;
    transform: translateY(4rpx);
}