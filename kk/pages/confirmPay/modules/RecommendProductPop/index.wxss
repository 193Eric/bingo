    background-image: url(data:image/png;base64,);
}

.recommend-product-popup.show .recommend-product-popup__bg {
    animation: fadeIn .4s forwards;
}

.recommend-product-popup.show .recommend-product-popup__container {
    animation: fadeInScale .4s forwards;
}

.recommend-product-popup.hide .recommend-product-popup__bg {
    opacity: 0;
}

.recommend-product-popup.hide .recommend-product-popup__container {
    animation: fadeOutScale .4s forwards;
}