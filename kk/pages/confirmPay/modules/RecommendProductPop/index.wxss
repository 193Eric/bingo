@-webkit-keyframes fadeInScale {
    0% {
        opacity: 0;
        transform: scale(0);
    }

    to {
        opacity: 1;
        transform: scale(1);
    }
}

@keyframes fadeInScale {
    0% {
        opacity: 0;
        transform: scale(0);
    }

    to {
        opacity: 1;
        transform: scale(1);
    }
}

@-webkit-keyframes fadeOutScale {
    0% {
        opacity: 1;
        transform: scale(1);
    }

    to {
        opacity: 0;
        transform: scale(0);
    }
}

@keyframes fadeOutScale {
    0% {
        opacity: 1;
        transform: scale(1);
    }

    to {
        opacity: 0;
        transform: scale(0);
    }
}

@-webkit-keyframes fadeIn {
    0% {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@keyframes fadeIn {
    0% {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@-webkit-keyframes fadeOut {
    to {
        opacity: 0;
    }
}

@keyframes fadeOut {
    to {
        opacity: 0;
    }
}

.recommend-product-popup,.recommend-product-popup__bg {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 100%;
    z-index: 1111;
}

.recommend-product-popup .c-modal__footer {
    padding: 30rpx 0;
    text-align: center;
    background-color: transparent;
}

.recommend-product-popup .c-modal__footer .o-icon-cancel {
    width: 64rpx;
    height: 64rpx;
    font-size: 64rpx;
    opacity: .8;
    color: #fff;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
    margin: 0 auto;
}

.recommend-product-popup.show-close__body {
    top: 50%;
}

.recommend-product-popup__bg {
    position: absolute;
    background: rgba(0,0,0,.3);
    z-index: 1112;
}

.recommend-product-popup__body {
    position: absolute;
    z-index: 1113;
    left: 50%;
    top: 43%;
    transform: translate3d(-50%,-50%,0);
}

.recommend-product-popup__body .close {
    position: absolute;
    left: 50%;
    bottom: -100rpx;
    margin-left: -40rpx;
    font-size: 60rpx;
    color: #fff;
}

.recommend-product-popup__container {
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
    width: 572rpx;
    height: 830rpx;
}

.recommend-product-popup .product-title {
    text-align: center;
    font-size: 32rpx;
    color: #fff;
    padding-top: 128rpx;
    padding-left: 80rpx;
    padding-right: 100rpx;
    height: 178rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.recommend-product-popup .product-container {
    position: relative;
    width: 320rpx;
    height: 394rpx;
    margin: 76rpx auto 0;
}

.recommend-product-popup .product-tag {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 44rpx;
}

.recommend-product-popup .product-stepper {
    height: 104rpx;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    align-content: center;
}

.recommend-product-popup .product-stepper .item_btn_box,.recommend-product-popup .product-stepper stepper>view {
    position: static;
}

.recommend-product-popup .product-stepper .item_btn_box .product_quantity,.recommend-product-popup .product-stepper stepper>view .product_quantity {
    width: 68rpx;
}

.recommend-product-popup .product-stepper .item_btn_box input,.recommend-product-popup .product-stepper stepper>view input {
    width: 68rpx;
    height: 40rpx;
    border-radius: 8rpx;
}

.recommend-product-popup .product-info {
    width: 360rpx;
    height: 112rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
    position: absolute;
    left: -20rpx;
    bottom: 4rpx;
    text-align: center;
}

.recommend-product-popup .product-info__name {
    line-height: 52rpx;
    height: 52rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-size: 24rpx;
    color: #fff;
    padding: 0 40rpx;
}

.recommend-product-popup .product-info__price .price-orange {
    color: #ff5400;
    display: inline-block;
    font-size: 36rpx;
}

.recommend-product-popup .product-info__price .price-orange .react-price__unit {
    font-size: 24rpx;
}

.recommend-product-popup .product-info__price .price-gray {
    display: inline-block;
    padding-left: 10rpx;
    color: #b6b6b6;
    font-size: 24rpx;
    text-decoration: line-through;
}

.recommend-product-popup .add-btn {
    height: 60rpx;
}

.recommend-product-popup .product-img {
    width: 320rpx;
    height: 320rpx;
    margin: 0 auto;
}

.recommend-product-popup .product-img image,.recommend-product-popup .product-img img {
    width: 100%;
    height: 100%;
    max-width: 100%;
}

.recommend-product-popup .type_label_icon {
    height: 36rpx;
    background-size: contain!important;
    background-repeat: no-repeat;
}

.recommend-product-popup .type_label_0 {
    background-image: url(data:image/png;base64,);
}

