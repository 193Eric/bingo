.activity-section-two {
    margin-bottom: 30rpx;
}

.activity-container {
    margin: 0 auto;
    display: flex;
    flex-direction: row;
}

.activity-container .activity_item {
    position: relative;
    flex: 1;
    width: 216rpx;
    margin-right: 10rpx;
    height: 260rpx;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: cover;
    justify-content: space-between;
    align-items: center;
    padding: 24rpx 0;
}

.activity-container .activity_item .item_title {
    font-family: PingFang-SC-Medium;
    font-size: 24rpx;
    color: #333;
    letter-spacing: 0;
    margin-bottom: 0;
    text-align: center;
}

.activity-container .activity_item .item_img-container {
    margin-top: -4rpx;
    text-align: center;
    height: 120rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    align-content: center;
}

.activity-container .activity_item .item_img-container image,.activity-container .activity_item .item_img-container img {
    display: inline-block;
    width: 120rpx;
}

.activity-container .activity_item .item_button-container {
    position: absolute;
    bottom: 24rpx;
    left: 50%;
    transform: translateX(-50%);
}

.activity-container .activity_item .item_button-container .button {
    display: inline-block;
    width: 128rpx;
    height: 44rpx;
    line-height: 44rpx;
    background-image: -webkit-gradient(linear,left top,right top,from(#fabe39),to(#ff8c00));
    background-image: linear-gradient(90deg,#fabe39 0,#ff8c00 100%);
    border-radius: 22rpx;
    font-size: 24rpx;
    color: #fff;
    font-family: PingFang-SC-Medium;
    letter-spacing: 0;
    text-align: center;
    border: 0;
}

.activity-container .activity_item:last-child {
    margin-right: 0;
}

.activity-container-box {
    margin: 0 auto;
    width: 700rpx;
    padding: 26rpx 18rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.activity-container-box .title {
    color: #333;
    text-align: center;
    margin-bottom: 20rpx;
}

.activity-container-box .title .span {
    font-size: 24rpx;
}

.activity-container-box .title .p {
    font-size: 30rpx;
    font-family: PingFang-SC-Medium;
}