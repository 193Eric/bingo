.bottom-banner {
    overflow-y: hidden;
    box-sizing: border-box;
    width: 700rpx;
    margin: 40rpx auto;
}

.bottom-banner #footer-banner-wrapper,.bottom-banner .swiper-container-wrapper {
    height: 100%;
}

.bottom-banner image,.bottom-banner img {
    width: 100%;
    height: 100%;
    border-radius: 8rpx;
    display: block;
}