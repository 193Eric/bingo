.counpon-container {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-wrap: wrap;
    margin-top: 20rpx;
}

.counpon-container .coupon-text {
    font-family: PingFangSC-Medium;
    font-size: 24rpx;
    color: #000;
    margin-right: 16rpx;
}

.counpon-container.disabled {
    background-image: -webkit-gradient(linear,left top,left bottom,color-stop(8%,#efefef),to(#d3d3d3));
    background-image: linear-gradient(-180deg,#efefef 8%,#d3d3d3 100%);
}

.counpon-container.disabled .li:not(:last-child) {
    border-right: 2rpx dashed #c6c6c6;
}

.counpon-container.disabled .coupon-button_item {
    display: flex;
}

.counpon-container.disabled .coupon-button_item button {
    color: #8a8a8a;
    padding-left: 26rpx;
    padding-right: 26rpx;
}

.counpon-container .coupon_item {
    display: flex;
    align-items: center;
    justify-content: flex-start;
    padding-left: 12rpx;
    padding-right: 12rpx;
    height: 40rpx;
    position: relative;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
    background-repeat: no-repeat;
    box-sizing: border-box;
    font-size: 22rpx;
    color: #fff1cf;
    font-weight: 700;
    text-align: center;
}

.counpon-container .coupon_item .value {
    font-weight: 700;
}

.counpon-container .coupon_item+.coupon_item {
    margin-left: 16rpx;
}

.counpon-container .coupon_item .prize-amount,.counpon-container .coupon_item .prize-name {
    white-space: nowrap;
    line-height: 36rpx;
    display: flex;
    align-items: center;
    font-weight: 400;
}

.counpon-container .coupon_item .prize-amount {
    font-size: 26rpx;
}

.counpon-container .coupon_item .prize-amount .discount_unit,.counpon-container .coupon_item .prize-name {
    font-size: 22rpx;
}

.counpon-container .coupon_item .prize-amount .unit {
    display: inline-block;
    font-size: 20rpx;
    vertical-align: text-bottom;
    margin-left: -6rpx;
    letter-spacing: -4rpx;
}

.counpon-container .desc {
    font-family: PingFangSC-Medium;
    font-size: 24rpx;
    color: #000;
    margin-left: 14rpx;
}

.counpon-container .desc .price {
    color: #f21d02;
}

.counpon-container .coupon-button_item {
    width: 128rpx;
    height: 44rpx;
    line-height: 44rpx;
    font-family: PingFangSC-Medium;
    font-size: 22rpx;
    color: #f21d02;
    letter-spacing: 0.26rpx;
    border: 0;
    box-sizing: border-box;
    background: #fff;
    box-shadow: 0 2rpx 4rpx 0 rgba(0,0,0,.15);
    border-radius: 48rpx;
    margin-left: 20rpx;
    margin-right: 0;
}

.counpon-container .coupon-button_item.disabled {
    color: rgba(138,138,138,.5);
    padding-left: 26rpx;
    padding-right: 26rpx;
}