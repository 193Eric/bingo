.coupon-modal {
    background-size: 100% 100%;
    width: 644rpx;
    box-sizing: border-box;
    padding-top: 0!important;
    margin: auto auto auto 38rpx;
}

.coupon-modal view {
    box-sizing: border-box;
    font-size: 28rpx;
}

.coupon-modal .font12,.coupon-modal .list .right .price .unit {
    font-size: 24rpx;
}

.coupon-modal .font14 {
    font-size: 28rpx;
}

.coupon-modal .font16 {
    font-size: 32rpx;
}

.coupon-modal .coupon-modal-header {
    width: 675rpx;
    max-width: 676rpx;
    display: block;
    margin: 0 auto;
    transform: translateX(-38rpx) translateY(2rpx);
}

.coupon-modal .coupon-modal-header image,.coupon-modal .coupon-modal-header img {
    max-width: 644rpx;
}

.coupon-modal .coupon-package-list {
    background: #e5183e;
    padding: 20rpx 54rpx;
    max-height: 440rpx;
    box-sizing: border-box;
    width: 598rpx;
}

.coupon-modal .list {
    width: 100%;
    height: 116rpx;
    background-color: #fff;
    padding: 24rpx 14rpx 0 20rpx;
    display: flex;
    border-radius: 10rpx;
    margin-bottom: 23rpx;
    position: relative;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
}

.coupon-modal .list:last-child {
    margin-bottom: 0;
}

.coupon-modal .list .p {
    color: #666;
}

.coupon-modal .list .em {
    display: block;
    line-height: 50rpx;
}

.coupon-modal .list .title {
    line-height: 28rpx;
    margin-bottom: 16rpx;
}

.coupon-modal .list .left {
    width: 314rpx;
    flex: 0 auto;
    position: relative;
}

.coupon-modal .list .left .em {
    font-family: PingFang-SC-Medium;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-weight: 500;
    color: #333;
}

.coupon-modal .list .right {
    width: 194rpx;
    text-align: center;
    padding-left: 28rpx;
}

.coupon-modal .list .right .em {
    color: #ff2b00;
}

.coupon-modal .list .right .price {
    color: #fe6722;
    font-size: 36rpx;
    line-height: 36rpx;
    margin-bottom: 6rpx;
    margin-top: -4rpx;
}

.coupon-modal .list .right .condition {
    color: #666;
    white-space: nowrap;
}

.coupon-modal .tip {
    color: #890019;
    line-height: 90rpx;
    background: url(data:image/png;base64,) no-repeat 150rpx center #e5183e;
    background-size: 34rpx 34rpx;
    border-bottom-left-radius: 40rpx;
    border-bottom-right-radius: 40rpx;
    text-indent: 60rpx;
    text-align: center;
    margin-top: -4rpx;
    width: 598rpx;
}

.coupon-modal .tip.coupon-record-tips {
    background-image: none;
    height: 90rpx;
}

.coupon-modal .coupon-date {
    color: #ff8d5a!important;
    font-size: 24rpx;
    line-height: 24rpx;
    display: flex;
    align-items: center;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.coupon-modal .coupon-date::before {
    content: "";
    display: inline-block;
    width: 24rpx;
    height: 24rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
    margin-right: 4rpx;
}