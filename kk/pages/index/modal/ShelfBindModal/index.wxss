.shelf_bind_window {
    width: 620rpx;
    background: #fff;
    border-radius: 40rpx;
    box-sizing: border-box;
    padding: 60rpx;
}

.shelf_bind_window .shelf_bind_icon {
    width: 320rpx;
    height: 320rpx;
    margin: 0 auto;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
}

.shelf_bind_window .shelf_bind_info {
    line-height: 1.5;
    font-size: 30rpx;
    font-weight: 400;
    color: #0d0d0d;
    text-align: center;
    margin-bottom: 40rpx;
    margin-top: 30rpx;
}

.shelf_bind_window .shelf_bind_btn {
    line-height: 96rpx;
    border-radius: 48rpx;
    background: #fabe39;
    font-size: 34rpx;
    color: #0d0d0d;
    text-align: center;
}