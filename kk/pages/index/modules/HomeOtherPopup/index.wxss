@-webkit-keyframes fadeIn {
    0% {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@keyframes fadeIn {
    0% {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@-webkit-keyframes fadeOut {
    0% {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}

@keyframes fadeOut {
    0% {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}

@-webkit-keyframes cardFlipIn {
    0% {
        transform: rotateY(-200deg) scale(0);
        opacity: 0;
    }

    to {
        transform: rotateY(0) scale(1);
        opacity: 1;
    }
}

@keyframes cardFlipIn {
    0% {
        transform: rotateY(-200deg) scale(0);
        opacity: 0;
    }

    to {
        transform: rotateY(0) scale(1);
        opacity: 1;
    }
}

@-webkit-keyframes cardFlipOut {
    to {
        transform: rotateY(-200deg) scale(0);
        opacity: 0;
    }
}

@keyframes cardFlipOut {
    to {
        transform: rotateY(-200deg) scale(0);
        opacity: 0;
    }
}

@-webkit-keyframes scaleDraw {
    0%,50% {
        transform: scale(1) translateX(-50%) translateY(-50%);
    }

    25%,75% {
        transform: scale(1.1) translateX(-50%) translateY(-50%);
    }
}

@keyframes scaleDraw {
    0%,50% {
        transform: scale(1) translateX(-50%) translateY(-50%);
    }

    25%,75% {
        transform: scale(1.1) translateX(-50%) translateY(-50%);
    }
}

.task-card-popup,.task-card-popup__bg {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 9990;
}

.task-card-popup__bg {
    position: absolute;
    background: rgba(0,0,0,.35);
    z-index: 99990;
}

.task-card-popup__content {
    position: fixed;
    left: 50%;
    top: 40%;
    z-index: 99999;
    margin-left: -300rpx;
    margin-top: -300rpx;
    width: 600rpx;
    min-height: 440rpx;
    background: #fff;
    border-radius: 16rpx;
    padding: 100rpx 40rpx 24rpx;
}

.task-card-popup__content .header-img {
    position: absolute;
    left: 50%;
    top: -60rpx;
    margin-left: -90rpx;
}

.task-card-popup__content .header-img image,.task-card-popup__content .header-img img {
    width: 178rpx;
    height: 148rpx;
}

.task-card-popup__content .content__title {
    font-family: PingFangSC-Medium;
    font-size: 36rpx;
    color: #3a3a3a;
    margin-bottom: 22rpx;
    text-align: center;
    font-weight: 700;
}

.task-card-popup__content .content__content {
    font-size: 28rpx;
}

.task-card-popup__content .content__content .pt {
    color: #333;
    margin-bottom: 10rpx;
}

.task-card-popup__content .content__content .p {
    text-indent: 60rpx;
    color: #6e6e6e;
    margin-bottom: 10rpx;
}

.task-card-popup__content .content__content .p .sp {
    margin-left: -8rpx;
}

.task-card-popup__content .content__content .text {
    color: #ff9600;
    padding: 0 4rpx;
}

.task-card-popup__content .content__btn {
    width: 492rpx;
    height: 96rpx;
    line-height: 96rpx;
    text-align: center;
    margin-top: 80rpx;
    margin-bottom: 20rpx;
    background: #fabe39;
    border-radius: 200rpx;
    font-size: 34rpx;
    color: #0d0d0d;
}

.task-card-popup__content .content__btn:active {
    opacity: .7;
}

.task-card-popup__content .content__tips {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #ff9600;
    text-align: center;
    text-decoration: underline;
}

.task-card-popup.show .task-card-popup__bg {
    animation: fadeIn .1s forwards;
}

.task-card-popup.show .task-card-popup__content {
    animation: cardFlipIn .6s forwards;
}

.task-card-popup.hide .task-card-popup__bg {
    animation: fadeOut .1s forwards;
}

.task-card-popup.hide .task-card-popup__content {
    animation: cardFlipOut .6s forwards;
}

.flip-container {
    perspective: 1000;
}

.task-card-popup .task-card-logo {
    position: absolute;
    width: 362rpx;
    height: 156rpx;
    top: -78rpx;
    left: 0;
    background-image: url(https://qn-test-pic.feng1.com/theme_pic-20210803-b24a6725e98961fe7de24c93368581a7?imageslim);
    background-size: 100% 100%;
    z-index: 2;
}

.task-card-popup .task-card-header {
    position: absolute;
    width: 100%;
    height: 106rpx;
    top: 0;
    left: 0;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.task-progress {
    width: 240rpx;
    height: 240rpx;
}

.task-progress-wrapper {
    position: relative;
    text-align: center;
    margin-top: 30rpx;
}

.task-progress-wrapper .task-progress-coupon-icon,.task-progress-wrapper .task-progress-desc {
    position: absolute;
    left: 50%;
    transform: translateX(-50%) translateY(-50%);
}

.task-progress-wrapper .task-progress-coupon-icon {
    width: 120rpx;
    height: 64rpx;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
    top: 50%;
    margin-top: -12rpx;
}

.task-progress-wrapper .task-progress-coupon-icon.active {
    transform-origin: 0 20rpx;
    background-image: url(data:image/png;base64,);
    animation: scaleDraw 5s ease-in-out infinite;
}

.task-progress-wrapper .task-progress-desc {
    bottom: 0;
    line-height: 28rpx;
    font-size: 24rpx;
}

.task-progress-wrapper .task-progress-desc .task-progress-count {
    font-family: PingFangSC-Medium;
    font-size: 24rpx;
    color: #282828;
}

.task-progress-wrapper .task-progress-desc .task-progress-text {
    font-family: PingFangSC-Regular;
    font-size: 20rpx;
    color: #ff9600;
}