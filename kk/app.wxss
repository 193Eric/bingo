.page-iphonex {
    height: calc(100vh - 64rpx)!important;
}

.bottom-fix-iphonex,.page-iphonex .bottom-sheet-content,.page-iphonex .confirm-pay,.page-iphonex .p-chart-footer,.page-iphonex.p-chart {
    bottom: 64rpx;
}

.page-iphonex .iphone-x-fix-white-padding {
    height: 64rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    background: #fff;
}

.fe-button,button {
    outline: 0;
    border: 0;
}

.fe-button::after,button::after {
    display: none;
}

.fe-button {
    width: 100%;
    height: 92rpx;
    background: #fabe39;
    box-shadow: 0 8rpx 24rpx 0 rgba(250,190,57,.5);
    border-radius: 48rpx;
    font-size: 34rpx;
    color: #0d0d0d;
    text-align: center;
    line-height: 92rpx;
    margin: 0 auto;
    font-weight: 1200rpx;
}

.fe-button:active {
    opacity: .8;
}

.counter-modal {
    width: 600rpx;
    min-height: 300rpx;
    background: #fff;
    border-radius: 8rpx;
    position: relative;
}

.counter-modal .btn {
    width: 476rpx;
    height: 96rpx;
    border-radius: 96rpx;
    margin: 0 auto;
    background: #fabe39;
    text-align: center;
    line-height: 96rpx;
    color: #0d0d0d;
    font-size: 34rpx;
}

.counter-modal,view {
    box-sizing: border-box;
}

.clearfix {
    clear: both;
    overflow: hidden;
}

.clearfix:after {
    clear: both;
    display: block;
    visibility: hidden;
    height: 0;
    content: " ";
    font-size: 0;
}

.product_content_item--combination {
    width: 324rpx;
    height: 324rpx;
    background: #fff;
    border-radius: 16rpx;
    display: inline-block;
    box-sizing: border-box;
    padding: 12.4rpx 20rpx 14rpx;
    position: relative;
}

.product_content_item--combination.product-placeholder {
    text-align: center;
    vertical-align: top;
    padding: 0;
    margin-left: 16rpx;
}

.product_content_item--combination.product-placeholder image,.product_content_item--combination.product-placeholder img {
    width: 94.8rpx;
    height: 100rpx;
    display: block;
    margin: 80rpx auto 6rpx;
}

.product_content_item--combination.product-placeholder .text {
    font-size: 26rpx;
    color: #4d4d4d;
    letter-spacing: 0.32rpx;
    line-height: 36rpx;
