}

.discount-card .discount-card-content,.discount-card .discount-card-header {
    display: flex;
    align-items: center;
    margin-bottom: 10rpx;
}

.discount-card .discount-card-header .left {
    font-size: 20rpx;
    line-height: 32rpx;
}

.discount-card .discount-card-header .right {
    font-size: 20rpx;
}

.discount-card .discount-card-content {
    font-size: 28rpx;
    color: #333;
}

.discount-card .discount-card-content .number {
    font-size: 36rpx;
    color: #ff9600;
    display: inline-block;
    margin: 0 4rpx;
    font-weight: 700;
}

.discount-card .discount-card-content .left {
    font-size: 28rpx;
    line-height: 32rpx;
    display: flex;
    align-items: center;
}

.discount-card .discount-card-content .left .rest {
    margin-left: 14rpx;
}

.discount-card .discount-card-content .right {
    font-size: 28rpx;
}

.discount-card .discount-card-footer {
    display: flex;
    align-items: center;
    font-size: 20rpx;
    color: #c4b7ae;
}