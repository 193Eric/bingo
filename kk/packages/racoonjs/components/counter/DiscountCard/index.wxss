@-webkit-keyframes arrowFlash {
    0% {
        color: #d8d8d8;
    }

    to {
        color: #4d4d4d;
    }
}

@keyframes arrowFlash {
    0% {
        color: #d8d8d8;
    }

    to {
        color: #4d4d4d;
    }
}

.anniversary-margin {
    overflow: initial!important;
}

.anniversary-progress {
    position: relative;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}

.anniversary-progress view {
    box-sizing: border-box;
}

.anniversary-progress .progress-block {
    display: flex;
    margin-top: 40rpx;
    margin-bottom: 40rpx;
    justify-content: space-between;
    position: relative;
}

.anniversary-progress .progress-block.progress-block--min {
    flex: 0 auto;
}

.anniversary-progress .progress-block:after {
    content: '';
    display: block;
    height: 6rpx;
    background: #feebc1;
    position: absolute;
    left: 0;
    top: 32rpx;
    right: 0;
    z-index: 1;
}

.anniversary-progress .progress-block__item {
    text-align: center;
    font-family: PingFang-SC-Medium;
    font-size: 26rpx;
    color: #999;
    position: relative;
    z-index: 3;
    margin-right: 24rpx;
}

.anniversary-progress .progress-block__item .check {
    width: 68rpx;
    height: 68rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
    position: relative;
    z-index: 5;
}

.anniversary-progress .progress-block__item.active::after {
    content: '';
    width: 360rpx;
    height: 6rpx;
    background: #ff8d00;
    position: absolute;
    top: 32rpx;
    left: -180rpx;
    z-index: 1;
}

.anniversary-progress .progress-block__item.active .check {
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}

.anniversary-progress .progress-block__item span {
    width: 68rpx;
    height: 48rpx;
    line-height: 54rpx;
    margin-top: 10rpx;
    display: block;
}

.anniversary-progress .progress-block__item span.active {
    width: 84rpx;
    margin-left: -8rpx;
    display: block;
    line-height: 54rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
    color: #fff;
}

.anniversary-progress .progress-block__item:first-child.active::after {
    width: 180rpx;
    left: 0;
}

.anniversary-progress .progress-block__item:last-child {
    margin-right: 0;
}

.anniversary-progress .progress-block__item:last-child.active::after {
    width: 180rpx;
    right: 12rpx;
}

.anniversary-progress .progress-block_leng2 .progress-block__item:first-child.active:after {
    width: 310rpx;
}

.anniversary-progress .theme1 {
    padding: 0 22rpx 30rpx;
    margin: 48rpx 24rpx;
    background: #fff;
    border-radius: 16rpx;
    min-height: 200rpx;
    position: relative;
    box-shadow: 0 0 16rpx 0 rgba(216,149,0,.29);
}

.anniversary-progress .theme1__title {
    display: flex;
    padding-top: 34rpx;
    height: 104rpx;
    box-sizing: border-box;
}

.anniversary-progress .theme1__title .ann-publicity-map {
    width: 104rpx;
    height: 104rpx;
    position: relative;
}

.anniversary-progress .theme1__title .ann-publicity-map image,.anniversary-progress .theme1__title .ann-publicity-map img {
    width: 104rpx;
    height: 104rpx;
    max-width: inherit;
    position: relative;
    top: -50rpx;
}

.anniversary-progress .theme1__title .title-wrapper {
    padding-left: 20rpx;
    flex: 1;
}

.anniversary-progress .theme1__title .main-title {
    font-size: 30rpx;
    color: #333;
    font-weight: 700;
    padding-top: 6rpx;
}

.anniversary-progress .theme1__title .small-title {
    font-size: 26rpx;
    color: #666;
}

.anniversary-progress .theme1__right_btn span,.anniversary-progress .theme1__title .button {
    border: 2rpx solid rgba(250,190,57,.8);
    border-radius: 47rpx;
    font-size: 24rpx;
    color: #ff9600;
    font-weight: 700;
    width: 160rpx;
    height: 56rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.anniversary-progress .theme1 .progress-block {
    margin: 32rpx 16rpx;
}

.anniversary-progress .theme1__right_btn {
    position: absolute;
    top: 24rpx;
    right: 32rpx;
}

.anniversary-progress .theme1__right_btn image,.anniversary-progress .theme1__right_btn img {
    width: 216rpx;
    margin-top: -6rpx;
}

.anniversary-progress .theme1__right_btn span {
    display: block;
}

.anniversary-progress .theme1 .p {
    font-size: 26rpx;
    color: #666;
    text-align: center;
    flex: 1;
}

.anniversary-progress .theme1 .p .price,.anniversary-progress .theme1 .p span {
    padding: 0 10rpx;
    font-weight: 700;
    font-size: 26rpx;
    color: #ff9600;
}

.anniversary-progress .theme2 {
    height: 48rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.anniversary-progress .theme2 .progress-block {
    margin: 0;
}

.anniversary-progress .theme2 .progress-block::after,.anniversary-progress .theme2 .progress-block__item:after {
    top: 24rpx;
}

.anniversary-progress .theme2 .progress-block__item .check {
    width: 52rpx;
    height: 52rpx;
}

.anniversary-progress .theme2 .progress-block__item.active::after {
    width: 88rpx;
    left: -24rpx;
}

.anniversary-progress .theme2 .progress-block__item:first-child.active::after {
    width: 62rpx;
    left: 0;
}

.anniversary-progress .theme2 .progress-block__item:last-child.active::after {
    width: 62rpx;
    right: 12rpx;
}

.anniversary-progress .theme2>div,.anniversary-progress .theme2>view {
    flex: 1;
}

.anniversary-progress .theme2 .ann-publicity-map {
    width: 68rpx;
    height: 68rpx;
    position: relative;
    flex: 0 auto;
    flex-shrink: 0;
}

.anniversary-progress .theme2 .ann-publicity-map image,.anniversary-progress .theme2 .ann-publicity-map img {
    width: 82rpx;
    height: 82rpx;
    position: absolute;
    left: -26rpx;
    top: -10rpx;
    max-width: inherit;
}

.anniversary-progress .theme2>.p {
    flex: 1;
    font-family: PingFangSC-Medium;
    font-size: 24rpx;
    color: #333;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.arrow-flash {
    padding-left: 20rpx;
    flex: initial;
    display: flex;
}

.arrow-flash .i {
    font-size: 18rpx;
    margin-left: -8rpx;
    margin-top: -.2 .5em;
    vertical-align: middle;
}

.arrow-flash .i:nth-child(1) {
    animation: arrowFlash 2s infinite;
}

.arrow-flash .i:nth-child(2) {
    animation: arrowFlash 2s .55s infinite;
    margin-left: -6rpx;
}

.progress-block .progress-block__item:after {
    opacity: 0;
}

.progress-block .progress-block__item .check,.progress-block--abs .progress-block__item:after {
    opacity: 1;
}

.progress-block--abs {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    display: flex;
    justify-content: space-between;
    z-index: 2;
    overflow: hidden;
}

.progress-block--abs .progress-block__item .check {
    opacity: 0;
}

.anniversary-progress .activity-complete .progress-block:after {
    background: #ff8d00;
}

.anniversary-progress .activity-complete .progress-block__item:after {
    display: none;
}

.activity-section-group {
    margin-bottom: 30rpx;
}

.activity-group-container {
    width: 700rpx;
    position: relative;
    display: flex;
    margin: 0 auto 4rpx;
    background-color: #fff;
    border-radius: 16rpx;
}

.activity-group-container .des-container {
    width: 520rpx;
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-content: center;
    position: relative;
    z-index: 2;
    padding-left: 20rpx;
}

.activity-group-container .title {
    margin-top: 10rpx;
    font-family: PingFang-SC-Heavy;
    font-size: 40rpx;
    letter-spacing: 0.42rpx;
    margin-bottom: 0;
    font-weight: 600;
}

.activity-group-container .title .des1 {
    color: #ff9600;
}

.activity-group-container .title .task {
    color: #333;
}

.activity-group-container .des-btn .des2 {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #999;
    letter-spacing: -0.6rpx;
}

.activity-group-container .img-container {
    width: 160rpx;
    flex: 0 auto;
    text-align: center;
}

.activity-group-container .img-container image,.activity-group-container .img-container img {
    width: 160rpx;
    height: 160rpx;
    border-top-right-radius: 16rpx;
    border-bottom-right-radius: 16rpx;
}

.discount-card-container {
    position: relative;
}

.discount-card-banner {
    margin: 0 20rpx;
}

.discount-card-banner .img {
    width: 100%;
}

.discount-card {
    color: #c4b7ae;
    width: 704rpx;
    height: 180rpx;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
    background-repeat: no-repeat;
    box-sizing: border-box;
    margin: 0 auto 24rpx;
}

.discount-card .discount-card-content-wrapper {
    padding: 24rpx 32rpx;
    box-sizing: border-box;
}

.discount-card .left {
