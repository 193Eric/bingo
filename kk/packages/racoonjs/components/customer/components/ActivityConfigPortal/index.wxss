.anniversary-progress-new .ann-content::after {
    content: "";
    background-image: linear-gradient(72deg,#fd8642 0,#c41dc7 91%);
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    max-width: 480rpx;
    z-index: -1;
    border-radius: 0 38rpx 38rpx 0;
}

.anniversary-progress-new .ann-content .num {
