@-webkit-keyframes upAnimation {
    0%,to {
        transform: rotate(0deg);
        transition-timing-function: cubic-bezier(.215,.61,.355,1);
    }

    10% {
        transform: rotate(-12deg);
        transition-timing-function: cubic-bezier(.215,.61,.355,1);
    }

    20% {
        transform: rotate(12deg);
        transition-timing-function: cubic-bezier(.215,.61,.355,1);
    }

    28% {
        transform: rotate(-10deg);
        transition-timing-function: cubic-bezier(.215,.61,.355,1);
    }

    36% {
        transform: rotate(10deg);
        transition-timing-function: cubic-bezier(.755,.5,.855,.06);
    }

    42% {
        transform: rotate(-8deg);
        transition-timing-function: cubic-bezier(.755,.5,.855,.06);
    }

    48% {
        transform: rotate(8deg);
        transition-timing-function: cubic-bezier(.755,.5,.855,.06);
    }

    52% {
        transform: rotate(-4deg);
        transition-timing-function: cubic-bezier(.755,.5,.855,.06);
    }

    56% {
        transform: rotate(4deg);
        transition-timing-function: cubic-bezier(.755,.5,.855,.06);
    }

    60% {
        transform: rotate(0deg);
        transition-timing-function: cubic-bezier(.755,.5,.855,.06);
    }
}

@keyframes upAnimation {
    0%,to {
        transform: rotate(0deg);
        transition-timing-function: cubic-bezier(.215,.61,.355,1);
    }

    10% {
        transform: rotate(-12deg);
        transition-timing-function: cubic-bezier(.215,.61,.355,1);
    }

    20% {
        transform: rotate(12deg);
        transition-timing-function: cubic-bezier(.215,.61,.355,1);
    }

    28% {
        transform: rotate(-10deg);
        transition-timing-function: cubic-bezier(.215,.61,.355,1);
    }

    36% {
        transform: rotate(10deg);
        transition-timing-function: cubic-bezier(.755,.5,.855,.06);
    }

    42% {
        transform: rotate(-8deg);
        transition-timing-function: cubic-bezier(.755,.5,.855,.06);
    }

    48% {
        transform: rotate(8deg);
        transition-timing-function: cubic-bezier(.755,.5,.855,.06);
    }

    52% {
        transform: rotate(-4deg);
        transition-timing-function: cubic-bezier(.755,.5,.855,.06);
    }

    56% {
        transform: rotate(4deg);
        transition-timing-function: cubic-bezier(.755,.5,.855,.06);
    }

    60% {
        transform: rotate(0deg);
        transition-timing-function: cubic-bezier(.755,.5,.855,.06);
    }
}

@-webkit-keyframes arrowFlash {
    0% {
        color: #fff;
    }

    to {
        color: #fff;
        opacity: .4;
    }
}

@keyframes arrowFlash {
    0% {
        color: #fff;
    }

    to {
        color: #fff;
        opacity: .4;
    }
}

.anniversary-progress-new .anniversary-progress-inner {
    height: 200rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    position: relative;
    display: flex;
    margin: 0 24rpx 40rpx;
}

.anniversary-progress-new .anniversary-progress-inner::after {
    content: "";
    width: 320rpx;
    height: 34rpx;
    background-image: linear-gradient(106deg,#fd8642 0,#ffce00 91%);
    border-radius: 26.6rpx;
    position: absolute;
    top: 14rpx;
    left: 318rpx;
    z-index: 1;
}

.anniversary-progress-new .gift-icon {
    position: absolute;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
    z-index: 14;
}

.anniversary-progress-new .gift-icon.finish {
    animation: upAnimation 2s infinite;
    animation-fill-mode: both;
    animation-iteration-count: infinite;
    animation-delay: 1s;
}

.anniversary-progress-new .gift-line {
    position: absolute;
    z-index: 13;
}

.anniversary-progress-new .ann-content {
    font-weight: 700;
    font-size: 30rpx;
    color: #fff;
    position: absolute;
    display: flex;
    align-items: center;
    padding-right: 30rpx;
    padding-left: 80rpx;
    border-radius: 0 38rpx 38rpx 0;
    z-index: 10;
    min-width: 0;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.anniversary-progress-new .ann-content span,.anniversary-progress-new .ann-content text {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    max-width: 540rpx;
