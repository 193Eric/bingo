<view wx:if="{{$taroCompReady}}">
    <modal compid="{{$compid__17}}">
        <toggle compid="{{$compid__18}}">
            <view class="new_user_register_window new_user_popup_content">
                <view class="show_icon"></view>
                <view class="show_title">为了您的账号安全，请先登录</view>
                <button bindgetphonenumber="onGetPhoneNumber" class="new_user_button" data-e-getphonenumber-so="this" id="new_user_button" openType="getPhoneNumber">立即登录</button>
            </view>
        </toggle>
        <toggle compid="{{$compid__19}}">
            <view class="coupon-streamer">
                <image alt="coupon-streamer" mode="widthFix" src="{{IMAGE_COUPON_STREAMER}}"></image>
            </view>
            <view class="new-user-info-container">
                <view class="coupon">
                    <toggle compid="{{$compid__20}}">
                        <view class="p">
                            <text class="strong new-user-coupon">{{price}}</text>
                            <text class="strong">元</text>优惠券</view>
                    </toggle>
                    <toggle compid="{{$compid__21}}">请先登录再购买</toggle>
                </view>
                <button bindgetphonenumber="onGetPhoneNumber" class="btn new-user-coupon-btn" data-e-getphonenumber-so="this" openType="getPhoneNumber">立即领取</button>
            </view>
        </toggle>
    </modal>
    <coupon-modal compid="{{$compid__22}}" wx:if="{{isShowCouponModal}}"></coupon-modal>
</view>
