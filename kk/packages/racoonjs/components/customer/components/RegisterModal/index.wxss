.new_user_register_window.new_user_popup_content {
    width: 584rpx;
    background: #fff;
    border-radius: 16rpx;
    padding: 40rpx 0;
}

.new_user_register_window.new_user_popup_content .show_icon {
    width: 162rpx;
    height: 130rpx;
    margin: 0 auto 20rpx;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
}

.new_user_register_window.new_user_popup_content .show_title {
    font-size: 32rpx;
    color: #333;
    letter-spacing: 0.04rpx;
    text-align: center;
    line-height: 80rpx;
    margin-bottom: 30rpx;
}

.new_user_register_window.new_user_popup_content .new_user_button {
    background: #fabe39;
    width: 492rpx;
    line-height: 96rpx;
    margin: 0 auto;
    font-size: 34rpx;
    color: #0d0d0d;
    letter-spacing: 0;
    text-align: center;
    border-radius: 48rpx;
    display: block;
}

.coupon-streamer {
    position: absolute;
    z-index: 2030;
    transform: translate(1.2%,-48%);
    width: 680rpx;
}

.coupon-streamer image,.coupon-streamer img {
    width: 95%;
}

.new-user-info-container {
    position: relative;
    padding-top: 240rpx;
    width: 572rpx;
    margin: 0 auto;
    height: 606rpx;
    z-index: 1100;
    text-align: center;
    color: #fff;
    background: url(https://qn-cdn-pic.feng1.com/theme_pic-20200824-537613659992b98ba6301ff51950ff3e?imageslim) no-repeat center;
    background-size: 100% 100%;
    box-sizing: border-box;
}

.new-user-info-container .coupon {
    width: 412rpx;
    height: 160rpx;
    margin: 0 auto;
    line-height: 160rpx;
    font-family: PingFang-SC-Medium;
    font-size: 48rpx;
    color: #f7340a;
    letter-spacing: 0;
    text-align: center;
    box-sizing: border-box;
}

.new-user-info-container .coupon .strong {
    font-weight: 700;
    font-size: 64rpx;
    color: #f7340a;
    letter-spacing: 0;
    text-align: left;
    line-height: 64rpx;
}

.new-user-info-container .btn {
    margin: 60rpx auto 0;
    width: 340rpx;
    height: 88rpx;
    border-radius: 100rpx;
    border: 0;
    background-image: linear-gradient(-126deg,#fff5af 0,#fde74b 100%);
    font-size: 32rpx;
    color: #c10508;
    letter-spacing: 0.7rpx;
    line-height: 88rpx;
    box-sizing: border-box;
}