<block wx:if="{{$taroCompReady}}">
    <view wx:if="{{price===undefined}}"></view>
    <view class="{{'react-price react-price__type_'+openType+' '+(className||'')}}" wx:else>
        <block wx:if="{{openType=='9'}}">
            <text class="react-price__value">{{anonymousState__temp}}</text>
            <text class="react-price__typeName">{{thirdInfoData.exchangeTypeName||''}}</text>
        </block>
        <block wx:else>
            <text class="react-price__unit" wx:if="{{!showUnitName}}">¥</text>
            <text class="react-price__value">{{anonymousState__temp2}}</text>
            <text class="react-price__unitName" wx:if="{{showUnitName}}">元</text>
        </block>
    </view>
</block>
