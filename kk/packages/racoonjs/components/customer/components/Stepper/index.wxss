.item_btn_box {
    position: absolute;
    width: 160rpx;
    height: 48rpx;
    right: 0;
    bottom: 0;
}

.item_btn_box.has_quantity .reduce_btn {
    display: block;
}

.item_btn_box .span,.item_btn_box span {
    float: right;
    width: 48rpx;
    height: 48rpx;
    display: inline-block;
}

.item_btn_box .product_quantity {
    width: 60rpx;
    line-height: 44rpx;
    text-align: center;
    float: right;
}

.item_btn_box .reduce_btn {
    display: inline-block!important;
}

.item_btn_box .product_quantity {
    display: inline-block;
    font-size: 28rpx;
}

.item_btn_box .product_quantity input {
    font-size: 28rpx;
    width: 60rpx;
    border: 0;
    text-align: center;
    border-radius: 4rpx;
}

.item_btn_box .product_quantity input::-webkit-inner-spin-button,.item_btn_box .product_quantity input::-webkit-outer-spin-button {
    -webkit-appearance: none!important;
    margin: 0;
}

.item_btn_box .product_quantity .text {
    width: inherit;
    height: inherit;
    float: inherit;
}

.item_btn_box .add_btn {
    background: url(data:image/png;base64,) no-repeat center;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
}

.item_btn_box .add_btn--disabled {
    filter: grayscale(100%);
}

.item_btn_box .reduce_btn {
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
}

.item_btn_box .reduce_btn--disabled {
    filter: grayscale(100%);
}

.item_btn_box--white .add_btn {
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
}

.item_btn_box--white .reduce_btn {
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
}

.item_btn_box--white2 .add_btn {
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
    width: 44rpx;
    height: 44rpx;
}

.item_btn_box--white2 .reduce_btn {
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
    width: 44rpx;
    height: 44rpx;
}

.item_btn_box.stepper-margin {
    width: auto;
}

.item_btn_box.stepper-margin .product_quantity {
    margin: 0 16rpx;
}