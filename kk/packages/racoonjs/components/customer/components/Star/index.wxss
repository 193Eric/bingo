.fe-star {
    display: flex;
    align-items: center;
    justify-content: center;
}

.fe-star .fe-star-item {
    width: 46rpx;
    height: 44rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.fe-star .fe-star-item+.fe-star-item {
    margin-left: 20rpx;
}

.fe-star .fe-star-item__full {
    background-image: url(data:image/png;base64,);
}

.fe-star .fe-star-item__zero {
    background-image: url(data:image/png;base64,);
}