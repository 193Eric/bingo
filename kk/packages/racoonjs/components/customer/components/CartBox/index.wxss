.opening .cart_opening_bg {
    display: block;
}

.opening .center_scan_btn_box {
    display: none;
}

.center_scan_btn_box {
    position: absolute;
    width: 198rpx;
    height: 140rpx;
    overflow: hidden;
    top: -100rpx;
    left: 50%;
    margin-left: -98rpx;
    z-index: 900;
    transition: .3s ease;
    transform-origin: center 80%;
}

.center_scan_btn_box.hide {
    transform: scale(0);
    opacity: 0;
}

.center_scan_btn {
    position: absolute;
    top: 0;
    left: 0;
    width: 198rpx;
    height: 198rpx;
    z-index: 1000;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
}

.center_scan_btn .circle_ {
    width: 186rpx;
    height: 186rpx;
    background-color: #fabe39;
    border-radius: 50%;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}

.center_scan_btn .circle_ .scan_icon_,.center_scan_btn .circle_ .scan_words_ {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
}

.center_scan_btn .circle_ .scan_icon_ {
    width: 68rpx;
    height: 64rpx;
    bottom: 88rpx;
    background: url(data:image/png;base64,) no-repeat center #fabe39;
    background-size: 100% 100%;
}

.center_scan_btn .circle_ .scan_words_ {
    bottom: 48rpx;
    width: 100%;
    text-align: center;
    font-size: 18rpx;
    color: #0d0d0d;
    font-weight: 300;
}

.cart_ui_box,.fixed_scan_bar {
    position: fixed;
    bottom: 30rpx;
    transition: all .3s;
}

.cart_ui_box {
    padding: 0 24rpx;
    width: 100%;
    left: 0;
    z-index: 998;
    box-sizing: border-box;
}

.cart_ui_box.scroll-hidden {
    opacity: 0;
    transform: translateY(200rpx);
}

.cart_ui_box .cart_ui_wrap {
    height: 100rpx;
    border-radius: 50rpx;
    background: #fabe39;
    box-shadow: 0 4rpx 12rpx 0 rgba(250,190,57,.5);
    position: relative;
    overflow: initial;
}

.cart_ui_box .cart_ui_wrap .cart_left {
    float: left;
    width: 148rpx;
    height: 100%;
    position: relative;
}

.cart_ui_box .cart_ui_wrap .cart_left .new_cart_icon {
    position: absolute;
    left: 60%;
    top: -30rpx;
    width: 126rpx;
    height: 130rpx;
    transform: translateX(-50%);
    transform-origin: 0 50%;
    background-image: url(data:image/png;base64,);
    background-size: 126rpx 130rpx;
    transition: all .1s ease-in;
}

.cart_ui_box .cart_ui_wrap .cart_left .new_cart_icon.active {
    transform: scale(.85) translateX(-50%);
}

.cart_ui_box .cart_ui_wrap .cart_left .new_cart_icon .cart_product_num {
    position: absolute;
    top: 8rpx;
    right: 12rpx;
    height: 32rpx;
    min-width: 32rpx;
    border-radius: 50%;
    padding: 0 8rpx;
    line-height: 32rpx;
    text-align: center;
    background: red;
    color: #fff;
    font-size: 20rpx;
    box-sizing: border-box;
}

.cart_ui_box .cart_ui_wrap .cart_left .new_cart_icon .cart_product_num.cart_product_num_two {
    border-radius: 20rpx;
}

.cart_ui_box .cart_ui_wrap .cart_right {
    float: right;
    width: 180rpx;
    height: 100%;
    border-top-right-radius: 50rpx;
    border-bottom-right-radius: 50rpx;
    background: -webkit-gradient(linear,left top,left bottom,from(#333),to(#0d0d0d));
    background: linear-gradient(to bottom,#333,#0d0d0d);
}

.cart_ui_box .cart_ui_wrap .cart_right .cart_to_pay {
    display: block;
    line-height: 100rpx;
    color: #fff;
    font-family: PingFangSC-Regular;
    font-size: 36rpx;
    text-align: center;
}

.cart_ui_box .cart_ui_wrap .cart_center {
    margin: 0 180rpx 0 148rpx;
    position: relative;
    z-index: 998;
}

.cart_ui_box .cart_ui_wrap .cart_center .cart_center_top {
    height: 50rpx;
    padding-top: 8rpx;
    display: flex;
    align-items: center;
    box-sizing: border-box;
}

.cart_ui_box .cart_ui_wrap .cart_center .cart_center_top .final_total_price_box {
    color: #0d0d0d;
    margin-right: 8rpx;
    font-size: 28rpx;
}

.cart_ui_box .cart_ui_wrap .cart_center .cart_center_top .final_total_price_box .rmb_icon {
    font-size: 22rpx;
}

.cart_ui_box .cart_ui_wrap .cart_center .cart_center_top .final_total_price_box .final_total_price {
    font-size: 34rpx;
}

.cart_ui_box .cart_ui_wrap .cart_center .cart_center_top .origin_total_price_box {
    color: #7c5e1c;
    font-size: 24rpx;
    text-decoration: line-through;
}

.cart_ui_box .cart_ui_wrap .cart_center .cart_center_bot {
    display: flex;
}

.cart_ui_box .cart_ui_wrap .cart_center .cart_center_bot .preferential_tips {
    color: #fff;
    display: flex;
    padding: 0 12rpx;
    line-height: 38rpx;
    border-radius: 20rpx;
    font-size: 22rpx;
    background: -webkit-gradient(linear,left top,left bottom,from(#ff8500),to(#ff6b00));
    background: linear-gradient(to bottom,#ff8500,#ff6b00);
    white-space: nowrap;
}

.fixed_scan_bar {
    width: 308rpx;
    height: 100rpx;
    left: 50%;
    margin-left: -154rpx;
    border-radius: 100rpx;
    background: url(data:image/png;base64,) no-repeat 56rpx center #fabe39;
    background-size: 40rpx 38rpx;
    text-align: center;
    line-height: 104rpx;
    font-size: 36rpx;
    z-index: 500;
    text-indent: 56rpx;
    box-shadow: 0 8rpx 24rpx rgba(250,190,57,.5);
    color: #181818;
    overflow: visible;
}

.fixed_scan_bar.scroll-hidden {
    opacity: 0;
    transform: translateY(200rpx);
}

.expired_container {
    background: #fff;
    width: 600rpx;
    border-radius: 16rpx;
    padding: 30rpx 0 0;
}

.expired_container .expired_title {
    font-weight: 700;
    font-size: 34rpx;
    color: #4d4d4d;
    line-height: 80rpx;
    text-align: center;
}

.expired_container .expired_words {
    font-size: 28rpx;
    color: #4d4d4d;
    padding: 8rpx 30rpx 0;
}

.expired_container .expired_img {
    display: block;
    width: 290rpx;
    height: 358rpx;
    margin: 24rpx auto;
    border-radius: 10rpx;
}

.expired_container .expired_btn {
    text-align: center;
    font-family: PingFang-SC-Medium;
    font-size: 36rpx;
    color: #ff9600;
    letter-spacing: -0.86rpx;
    line-height: 112rpx;
    border-bottom-left-radius: 16rpx;
    border-bottom-right-radius: 16rpx;
}

.expired_container .expired_btn:active {
    background: #eee;
}

.preferential-price {
    margin-right: 8rpx;
    display: flex;
}

#new_cart_icon_empty,.new_cart_icon_empty {
    position: fixed;
    left: 44rpx;
    bottom: 32rpx;
    width: 126rpx;
    height: 130rpx;
}