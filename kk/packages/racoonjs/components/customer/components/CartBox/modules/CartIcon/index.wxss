.new_cart_icon {
    position: fixed;
    left: 20rpx;
    bottom: 20rpx;
    width: 126rpx;
    height: 130rpx;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
    transition: all .1s ease-in;
    z-index: 2000;
}

.new_cart_icon.active {
    transform: scale(.85);
}

.new_cart_icon .cart_product_num {
    position: absolute;
    top: 8rpx;
    right: 12rpx;
    height: 32rpx;
    min-width: 32rpx;
    border-radius: 20rpx;
    padding: 0 8rpx;
    line-height: 32rpx;
    text-align: center;
    background: red;
    color: #fff;
    font-size: 20rpx;
}