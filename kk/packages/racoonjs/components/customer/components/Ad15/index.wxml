<block wx:if="{{$taroCompReady}}">
    <view wx:if="{{anonymousState__temp}}"></view>
    <movable-area class="fixed-ad15-container" wx:else>
        <movable-view bindchange="onChange" bindtap="handleClick" bindtouchend="onTouchEnd" class="{{ad15Classname}}" data-e-change-so="this" data-e-tap-so="this" data-e-touchend-so="this" direction="all" x="{{initX}}" y="{{initY}}" wx:if="{{hasInit}}">
            <view bindtap="handleClick" data-e-tap-so="this">
                <image src="{{ad15.imgUrl}}"></image>
            </view>
        </movable-view>
    </movable-area>
</block>
