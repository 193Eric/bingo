.error-img {
    width: 240rpx;
    height: 240rpx;
    margin-bottom: 42rpx;
}

.absolute-error {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 390rpx;
    transform: translate(-50%,-50%);
    font-size: 28rpx;
    line-height: 40rpx;
    color: #868686;
    letter-spacing: 0.34rpx;
    text-align: center;
}