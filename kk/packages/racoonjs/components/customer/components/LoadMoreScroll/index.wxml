<template name="renderLoading">
    <view class="load-more__indicator">
        <view class="at-loading">
            <view class="at-loading__ring"></view>
            <view class="at-loading__ring"></view>
            <view class="at-loading__ring"></view>
        </view>
        <view class="content">加载中</view>
    </view>
</template>
<view class="{{className}}" wx:if="{{$taroCompReady}}">
    <slot></slot>
    <slot name="empty" wx:if="{{noData}}"></slot>
    <view class="load-more-tips">
        <template is="renderLoading" data="{{...anonymousState__temp}}" wx:if="{{loading}}"></template>
        <slot name="nomore" wx:if="{{noMore&&!noData}}"></slot>
        <view bindtap="retryFetch" data-e-tap-so="this" wx:if="{{hasError}}">加载失败，点击重试</view>
    </view>
</view>
