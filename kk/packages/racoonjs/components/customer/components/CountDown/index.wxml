<block wx:if="{{$taroCompReady}}">
    <view wx:if="{{isEnd}}"></view>
    <view class="count-down-time type_timer" wx:else>
        <view class="{{'time-container '+theme}}">
            <toggle compid="{{$compid__27}}">
                <text class="day">{{days}}å¤©</text>
            </toggle>
            <text class="time-box">{{hours}}</text>
            <text class="time-padding">:</text>
            <text class="time-box">{{minutes}}</text>
            <text class="time-padding">:</text>
            <text class="time-box">{{second}}</text>
            <toggle compid="{{$compid__28}}">
                <text class="time-box">{{millisecond}}</text>
            </toggle>
        </view>
    </view>
</block>
