.nowrap {
    white-space: nowrap;
}

.loading-wrapper {
    text-align: center;
    padding: 60rpx 0;
}

.loading {
    width: 102rpx;
    height: 104rpx;
    animation: loading .6s linear infinite;
}

.clearfix::after {
    clear: both;
    display: block;
    visibility: hidden;
    height: 0;
    content: " ";
    font-size: 0;
}

.clearfix {
    zoom: 1;
}

.page_common_margin {
    margin-right: 20rpx;
    margin-left: 20rpx;
    border-radius: 16rpx;
}

.fixed_bottom {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
}

.add_required::before {
    content: '*';
    color: #f5222d;
    margin-right: 6rpx;
}

.add_un_required::before {
    content: '*';
    visibility: hidden;
    margin-right: 6rpx;
}

.button-none {
    background: 0 0;
    display: inline;
    color: inherit;
    padding: 0;
    margin: 0;
    line-height: initial;
    font-size: inherit;
}

.pay_password_box {
    box-sizing: border-box;
}

.pay_password_box .pay_password_ul {
    width: 608rpx;
    margin: 0 auto;
    position: relative;
    border: 2rpx solid #f5f5f5;
    padding: 0;
}

.pay_password_box .pay_password_ul .pay_password_li {
    float: left;
    width: 100rpx;
    height: 100rpx;
    background: #fff;
    text-align: center;
    color: #000;
    line-height: 100rpx;
    box-sizing: border-box;
    font-size: 36rpx;
    position: relative;
    list-style: none;
    border-right: 2rpx solid #f5f5f5;
}

.pay_password_box .pay_password_ul .pay_password_li:last-child {
    border-right: none;
}

.pay_password_box .pay_password_ul .pay_password_li.has_value:before {
    content: "";
    width: 20rpx;
    height: 20rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    background: #000;
    border-radius: 50%;
    transform: translate(-50%,-50%);
}

.pay_password_box .pay_password_ul .pay_password_li.has_bot_border:before {
    content: "";
    width: 100%;
    height: 2rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    background: #f5f5f5;
}

.pay_password_box .pay_password_ul .pay_password_li.has_right_border:after {
    content: "";
    width: 2rpx;
    height: 100%;
    position: absolute;
    bottom: 0;
    right: 0;
    background: #f5f5f5;
}

.pay_password_box.balace_pay .pay_password_ul {
    width: 488rpx;
    margin: 0 auto;
}

.pay_password_box.balace_pay .pay_password_ul .pay_password_li {
    width: 80rpx;
    height: 80rpx;
}