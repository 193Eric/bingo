<view wx:if="{{$taroCompReady}}">
    <toggle compid="{{$compid__56}}">
        <view class="c-number-keyboard-mask"></view>
    </toggle>
    <view class="{{'c-number-keyboard  '+(!isShow?'hidden':'')}}">
        <toggle compid="{{$compid__57}}">
            <view class="c-number-keyboard__header">
                <view class="c-number-keyboard__apply" id="js-numberKeyApply">确定</view>
            </view>
        </toggle>
        <view class="clearfix">
            <view bindtap="clickKeyboard" class="{{'c-number-keyboard__key '+item.className}}" data-e-tap-a-a="{{item}}" data-e-tap-so="this" wx:for="{{numberMap}}" wx:for-index="_anonIdx2" wx:key="number">	{{item.showValue}}	</view>
        </view>
    </view>
</view>
