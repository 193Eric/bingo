<block wx:if="{{$taroCompReady}}">
    <view wx:if="{{!data||!data.length}}"></view>
    <view class="{{anonymousState__temp}}" style="{{anonymousState__temp2}}" wx:else>
        <swiper autoplay="{{true}}" bindchange="onChange" circular="{{true}}" class="swiper-container-wrapper" data-e-change-so="this" indicatorDots="{{finalIndicatorDots}}">
            <block wx:if="{{data}}">
                <swiper-item wx:for="{{loopArray31}}" wx:key="advertionId">
                    <view bindtap="onClickBanner" data-e-tap-a-a="{{item[$original]}}" data-e-tap-a-b="{{index}}" data-e-tap-so="this">
                        <image bindload="anonymousFunc0" data-e-load-a-a="{{item[_$indexKey]}}" data-e-load-so="this" id="{{'bannerImage'+index}}" mode="widthFix" src="{{item[$original].imgUrl}}"></image>
                    </view>
                </swiper-item>
            </block>
        </swiper>
    </view>
</block>
