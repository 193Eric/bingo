<template name="renderButton">
    <button bindgetphonenumber="funPrivatebczzz" class="{{'c-btn button-none '+(button.type?'btn-'+button.type:'btn-default')}}" openType="{{button.openType}}" wx:if="{{button.openType}}">{{button.label}}</button>
    <button bindtap="handleButtonClick" class="{{'c-btn button-none '+(button.disabled?'btn-disabled':button.type?'btn-'+button.type:'btn-default')}}" data-e-tap-a-a="{{button}}" data-e-tap-a-b="{{index}}" data-e-tap-so="this" wx:else>{{button.label}}</button>
</template>
<view catchtouchmove="handleTouchMove" class="{{dialogClass}}" wx:if="{{$taroCompReady}}">
    <view bindtap="handleClickOverlay" catchtouchmove="maskTouchMove" class="c-mask"></view>
    <view class="c-modal-body">
        <toggle compid="{{$compid__15}}">
            <view class="title" wx:if="{{!!title}}">{{title}}</view>
        </toggle>
        <view class="{{'content '+(title?'has-title':'')}}">
            <block wx:if="{{content}}">{{content}}</block>
            <slot wx:else></slot>
        </view>
        <view class="c-dialog-opt">
            <block wx:if="{{_buttons}}">
                <template is="renderButton" data="{{...button[$loopState__temp2]}}" wx:for="{{loopArray0}}" wx:for-item="button" wx:key="type"></template>
            </block>
        </view>
    </view>
</view>
