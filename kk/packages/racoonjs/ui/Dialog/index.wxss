.fe-dialog,.fe-dialog .c-mask {
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    transition: all .3s;
}

.fe-dialog {
    position: fixed;
    z-index: 1000;
    visibility: hidden;
}

.fe-dialog .c-mask {
    position: absolute;
    z-index: 1001;
    background: rgba(0,0,0,.6);
    opacity: 0;
}

.fe-dialog.c-dialog-open {
    visibility: visible;
}

.fe-dialog.c-dialog-open .c-mask {
    opacity: 1;
}

.fe-dialog.c-dialog-open .c-modal-body {
    opacity: 1;
    transform: translate(-50%,-65%);
}

.fe-dialog .c-modal-body,.fe-dialog.c-hide {
    opacity: 0;
    transition: all .3s;
}

.fe-dialog .c-modal-body {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-80%);
    z-index: 1002;
    width: 600rpx;
    background-color: #fff;
    border-radius: 10rpx;
}

.fe-dialog .c-modal-body .title {
    font-size: 36rpx;
    text-align: center;
    color: #4d4d4d;
    padding: 46rpx 32rpx 24rpx;
    font-family: PingFangSC-Medium;
}

.fe-dialog .c-modal-body .content {
    font-size: 30rpx;
    color: #353535;
    padding: 70rpx 40rpx 50rpx;
    text-align: center;
    word-wrap: break-word;
    word-break: break-all;
}

.fe-dialog .c-modal-body .content.has-title {
    padding-top: 0;
    color: #868686;
}

.fe-dialog .c-dialog-opt {
    height: 96rpx;
    display: flex;
    justify-content: space-around;
}

.fe-dialog .c-dialog-opt .c-btn {
    display: block;
    flex: 1;
    width: 100%;
    text-decoration: none;
    border-radius: 0;
    padding: 8rpx 0 6rpx;
    border: 0;
    border-top: 2rpx solid #f9f9f9;
    font-size: 32rpx;
}

.fe-dialog .c-dialog-opt .c-btn::after {
    display: none;
}

.fe-dialog .c-dialog-opt .c-btn:first-child {
    border-bottom-left-radius: 8rpx;
}

.fe-dialog .c-dialog-opt .c-btn:last-child {
    border-top: 0;
    border-bottom-right-radius: 8rpx;
}

.fe-dialog .c-dialog-opt .c-btn:active {
    opacity: .6;
}

.fe-dialog.one-button-style .c-modal-body {
    box-sizing: border-box;
    padding: 50rpx 60rpx;
}

.fe-dialog.one-button-style .c-modal-body .title {
    padding: 0;
}

.fe-dialog.one-button-style .c-modal-body .content {
    padding: 30rpx 74rpx 40rpx;
}

.fe-dialog.one-button-style .c-dialog-opt .c-btn {
    display: flex;
    align-items: center;
    justify-content: center;
    box-sizing: border-box;
    height: 88rpx;
    border-radius: 44rpx;
}

.fe-dialog .btn-default {
    border-top: 2rpx solid #f9f9f9;
    background: #fff;
    color: #3a3a3a;
    border-bottom-left-radius: 10rpx;
}

.fe-dialog .btn-primary {
    background: #fabe39;
    color: #0d0d0d;
    border-bottom-right-radius: 10rpx;
}

.fe-dialog .btn-textcolor {
    background: #fff;
    font-family: PingFang-SC-Medium;
    border-top: 2rpx solid #e7e7e8!important;
    font-size: 36rpx;
    color: #fabe39!important;
}

.fe-dialog .btn-disabled {
    background: #eaeaea;
    color: #333;
}

.fe-dialog .btn-disabled:active {
    opacity: 1!important;
}

.yuanqi-theme .btn-primary {
    background: #97d700;
}